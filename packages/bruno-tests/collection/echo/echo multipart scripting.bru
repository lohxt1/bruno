meta {
  name: echo multipart via scripting
  type: http
  seq: 10
}

post {
  url: {{echo-host}}
  body: multipartForm
  auth: none
}

assert {
  res.body: contains bar
}

script:pre-request {
  const FormData = require("form-data");
  const form = new FormData();
  form.append('foo', 'bar');
  req.setBody(form);
}
